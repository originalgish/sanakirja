{
  "name": "sanakirja",
  "version": "1.0.0",
  "author": "DenisM",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently --kill-others-on-fail \"yarn workspace @sanakirja/server dev\"  \"yarn workspace @sanakirja/client dev\"",
    "reinstall": "rm -rf node_modules packages/*/node_modules  && yarn"
  },
  "dependencies": {
    "concurrently": "^7.6.0"
  }
}
